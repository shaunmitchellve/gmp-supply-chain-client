{
  "extends": [
    "next/core-web-vitals",
    "eslint-config-next"
  ],
  "rules": {
    "node/no-unpublished-import": ["error", {
      "allowModules": ["tailwindcss", "jest", "@testing-library/react", "@testing-library/jest-dom"]
    }]
  },
  "plugins": ["jest", "node"],
  "env": {
    "jest/globals": true
  }
}
